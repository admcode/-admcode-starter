{
    "name": "admcode/admcode-starter",
    "description": "Create PHP projects based on ADMCode's template",
    "keywords": ["framework", "laravel", "lumen", "admcode", "admcode-starter"],
    "homepage": "https://github.com/admcode/admcode-starter",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0",
        "sebastian/phpcpd": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    }
}
